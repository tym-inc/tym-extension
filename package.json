{
	"name": "tym",
	"displayName": "Tym",
	"description": "Seamless code collaboration",
	"version": "1.0.0",
	"publisher": "tym",
	"repository": "https://github.com/tym-inc/tym-extension",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
				"id": "tym",
				"title": "Tym",
				"icon": "media/tym.svg"
				}
			]
		},
		"views": {
			"tym": [
				{
					"type": "webview",
					"id": "tym.collaborationView",
					"name": "Tym"
				}
			]
		}
	},
	"extensionDependencies": ["vscode.git"],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.5.5"
	},
	"icon": "https://firebasestorage.googleapis.com/v0/b/tokyo-unity-328718.appspot.com/o/tym-128.png?alt=media&token=c348cb5b-7d1f-46b6-8291-5b25351a8ed7"
}
